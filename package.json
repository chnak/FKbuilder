{
  "name": "fkbuilder",
  "version": "1.1.38",
  "description": "基于 Node.js + Paper.js 的程序化视频生成库，支持多轨道多场景、丰富动画效果、转场特效和并行渲染",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "index.js",
    "README.md"
  ],
  "scripts": {
    "build": "node build.js",
    "build:clean": "node build.js --clean",
    "build:watch": "node build.js --watch",
    "prepublishOnly": "npm run build",
    "start": "node examples/basic-composition.js",
    "example:basic": "node examples/basic-composition.js",
    "example:text": "node examples/text-animation.js",
    "example:image": "node examples/image-slideshow.js",
    "test": "node test-spritejs-basic.js",
    "test:fit": "node examples/test-fit-parameters.js"
  },
  "keywords": [
    "video",
    "animation",
    "canvas",
    "paper.js",
    "webgl",
    "ffmpeg",
    "creatomate",
    "editly"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "canvas": "^3.2.0",
    "cheerio": "1.0.0",
    "echarts": "^6.0.0",
    "execa": "^5.1.1",
    "font-finder": "^1.1.0",
    "font-list": "^2.0.1",
    "fs-extra": "^11.2.0",
    "gl": "^8.1.6",
    "gl-buffer": "^2.1.2",
    "gl-shader": "^4.3.1",
    "gl-texture2d": "^2.1.0",
    "gl-transition": "^1.13.0",
    "gl-transitions": "^1.43.0",
    "got": "^11.8.5",
    "jsdom": "^16.2.2",
    "lrc-kit": "^1.1.1",
    "nanoid": "^5.0.4",
    "paper": "^0.12.18",
    "spritejs": "^3.8.3",
    "zod": "^4.1.13"
  },
  "overrides": {
    "canvas": "^3.2.0"
  },
  "resolutions": {
    "**/canvas": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "eslint": "^8.57.0",
    "rollup": "^4.53.2",
    "rollup-plugin-node-externals": "^8.1.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
